    "test:lint": "eslint --ext .ts && prettier --check 'src/**/*.ts'",
    "fix:lint": "eslint --fix",
    "prettier-format": "prettier --config .prettierrc 'src/**/*.ts' --write",
    "tsc": "tsc"
  },
  "author": "Udacity",
  "license": "ISC",
  "dependencies": {
    "bcrypt": "^5.1.0",
    "body-parser": "^1.19.0",
